{
  "name": "bentools/funnel-http-client",
  "description": "A throttler for symfony/http-client to handle rate limits",
  "keywords": [
    "http",
    "client",
    "symfony",
    "httpclient",
    "decorator",
    "throttler",
    "throttling",
    "rate-limit"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Beno!t POLASZEK"
    }
  ],
  "require": {
    "php": ">=8.0",
    "symfony/http-client": "~5.0|~6.0",
    "psr/log": "^1|^2|^3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "~3.7",
    "symfony/var-dumper": "~5.0",
    "php-coveralls/php-coveralls": "^2.5",
    "phpstan/phpstan": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "BenTools\\FunnelHttpClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BenTools\\FunnelHttpClient\\Tests\\": "tests"
    },
    "files": [
      "vendor/symfony/var-dumper/Resources/functions/dump.php"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
